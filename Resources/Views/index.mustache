{{>head}}

<body>
  <div class="container">
    {{>header}}

    <div class="row" style="padding:48px 0 16px 0">
      <div class="col-xs-offset-3 col-xs-6">
        <h3 class="text-center">Authorize GitHub Access</h3>
        <h5>Starred Search requests no permissions other than read access to public data.</h5>
        <h5>Authorization is used to identify your account and avoid rate-limited anonymous GitHub API requests.</h5>
      </div>
    </div>

    <div class="row">
      <div class="col-xs-12 text-center">
        <button class="btn btn-success" onclick="window.location='{{url}}'">Authorize</button>
      </div>
    </div>

    {{>footer}}
  </div>
</body>
